<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <key n="68">
    <name>D</name>
    <desc>Passenger door</desc>
    <binding>
      <command>nasal</command>
      <script>carreidas.doorsystem.passengerexport();</script>
    </binding>
  </key>

  <key n="100">
    <name>d</name>
    <desc>Crew door</desc>
    <binding>
      <command>nasal</command>
      <script>carreidas.doorsystem.crewexport();</script>
    </binding>
  </key>

  <key n="127">
    <name>Delete</name>
    <desc>Toggle Reversers</desc>
    <binding>
      <command>property-toggle</command>
      <property>controls/engines/engine[0]/reverser</property>
    </binding>
    <binding>
      <command>property-toggle</command>
      <property>controls/engines/engine[2]/reverser</property>
    </binding>
  </key>

            <key n="267">
