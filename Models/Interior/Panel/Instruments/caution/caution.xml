<?xml version = '1.0' encoding = 'UTF-8' ?>
<PropertyList>
<path>caution.ac</path>
<animation>
  <type>pick</type>
  <object-name>button</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-assign</command>
      <property>/instrumentation/annunciators/master-caution</property>
      <value>0</value>
    </binding>
    <binding>
      <command>property-assign</command>
      <property>/instrumentation/annunciators/master-caution-button-status</property>
      <value>1</value>
    </binding>
    <mod-up>
      <binding>
        <command>property-assign</command>
      <property>/instrumentation/annunciators/master-caution-button-status</property>
        <value>0</value>
      </binding>
    </mod-up>
  </action>
</animation>
<animation>
    <type>material</type>
    <object-name>button</object-name>
    <emission>
      <red>   0.804 </red>
      <green> 0.584 </green>
      <blue>  0 </blue>
      <factor-prop>/instrumentation/annunciators/master-caution</factor-prop>
    </emission>
</animation>
<animation>
  <type>translate</type>
    <object-name>button</object-name>
  <factor>-0.0007</factor>
  <property>/instrumentation/annunciators/master-caution-button-status</property>
  <axis>
    <x>1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>
</PropertyList>
