<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>accel.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>button</object-name>
    <object-name>face</object-name>
    <object-name>needle-min</object-name>
    <object-name>needle</object-name>
    <object-name>needle-max</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <condition>
      <property>sim/rendering/rembrandt/enabled</property>
    </condition>
    <object-name>vitre</object-name>
  </effect>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle-min</object-name>
    <object-name>needle</object-name>
    <object-name>needle-max</object-name>
    <emission>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>accelerations/pilot-g</property>
    <interpolation>
      <entry><ind> -5 </ind><dep> -202.5 </dep></entry>
      <entry><ind> -4 </ind><dep> -112.5 </dep></entry>
      <entry><ind> -3 </ind><dep>  -90.0 </dep></entry>
      <entry><ind> -1 </ind><dep>  -45.0 </dep></entry>
      <entry><ind>  0 </ind><dep>  -22.5 </dep></entry>
      <entry><ind>  1 </ind><dep>    0.0 </dep></entry>
      <entry><ind>  3 </ind><dep>   45.0 </dep></entry>
      <entry><ind>  5 </ind><dep>   90.0 </dep></entry>
      <entry><ind>  7 </ind><dep>  135.0 </dep></entry>
      <entry><ind>  9 </ind><dep>  180.0 </dep></entry>
      <entry><ind> 10 </ind><dep>  202.5 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle-max</object-name>
    <property>sim/model/instrumentation/g-meter/g-max</property>
    <interpolation>
      <entry><ind> -5 </ind><dep> -202.5 </dep></entry>
      <entry><ind> -4 </ind><dep> -112.5 </dep></entry>
      <entry><ind> -3 </ind><dep>  -90.0 </dep></entry>
      <entry><ind> -1 </ind><dep>  -45.0 </dep></entry>
      <entry><ind>  0 </ind><dep>  -22.5 </dep></entry>
      <entry><ind>  1 </ind><dep>    0.0 </dep></entry>
      <entry><ind>  3 </ind><dep>   45.0 </dep></entry>
      <entry><ind>  5 </ind><dep>   90.0 </dep></entry>
      <entry><ind>  7 </ind><dep>  135.0 </dep></entry>
      <entry><ind>  9 </ind><dep>  180.0 </dep></entry>
      <entry><ind> 10 </ind><dep>  202.5 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle-min</object-name>
    <property>sim/model/instrumentation/g-meter/g-min</property>
    <interpolation>
      <entry><ind> -5 </ind><dep> -202.5 </dep></entry>
      <entry><ind> -4 </ind><dep> -112.5 </dep></entry>
      <entry><ind> -3 </ind><dep>  -90.0 </dep></entry>
      <entry><ind> -1 </ind><dep>  -45.0 </dep></entry>
      <entry><ind>  0 </ind><dep>  -22.5 </dep></entry>
      <entry><ind>  1 </ind><dep>    0.0 </dep></entry>
      <entry><ind>  3 </ind><dep>   45.0 </dep></entry>
      <entry><ind>  5 </ind><dep>   90.0 </dep></entry>
      <entry><ind>  7 </ind><dep>  135.0 </dep></entry>
      <entry><ind>  9 </ind><dep>  180.0 </dep></entry>
      <entry><ind> 10 </ind><dep>  202.5 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
