<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>A-10-accel.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>button</object-name>
    <object-name>face</object-name>
    <object-name>needle-min</object-name>
    <object-name>needle</object-name>
    <object-name>needle-max</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle-min</object-name>
    <object-name>needle</object-name>
    <object-name>needle-max</object-name>
    <emission>
      <red>   0.03125 </red>
      <green> 0.00890 </green>
      <blue>  0.00890 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>accelerations/pilot-g</property>
    <interpolation>
      <entry><ind> -5 </ind><dep> -202.5 </dep></entry>
      <entry><ind> -4 </ind><dep> -112.5 </dep></entry>
      <entry><ind> -3 </ind><dep>  -90.0 </dep></entry>
      <entry><ind> -1 </ind><dep>  -45.0 </dep></entry>
      <entry><ind>  0 </ind><dep>  -22.5 </dep></entry>
      <entry><ind>  1 </ind><dep>    0.0 </dep></entry>
      <entry><ind>  3 </ind><dep>   45.0 </dep></entry>
      <entry><ind>  5 </ind><dep>   90.0 </dep></entry>
      <entry><ind>  7 </ind><dep>  135.0 </dep></entry>
      <entry><ind>  9 </ind><dep>  180.0 </dep></entry>
      <entry><ind> 10 </ind><dep>  202.5 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </center> 
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle-max</object-name>
    <property>sim/model/A-10/instrumentation/g-meter/g-max</property>
    <interpolation>
      <entry><ind> -5 </ind><dep> -202.5 </dep></entry>
      <entry><ind> -4 </ind><dep> -112.5 </dep></entry>
      <entry><ind> -3 </ind><dep>  -90.0 </dep></entry>
      <entry><ind> -1 </ind><dep>  -45.0 </dep></entry>
      <entry><ind>  0 </ind><dep>  -22.5 </dep></entry>
      <entry><ind>  1 </ind><dep>    0.0 </dep></entry>
      <entry><ind>  3 </ind><dep>   45.0 </dep></entry>
      <entry><ind>  5 </ind><dep>   90.0 </dep></entry>
      <entry><ind>  7 </ind><dep>  135.0 </dep></entry>
      <entry><ind>  9 </ind><dep>  180.0 </dep></entry>
      <entry><ind> 10 </ind><dep>  202.5 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </center> 
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle-min</object-name>
    <property>sim/model/A-10/instrumentation/g-meter/g-min</property>
    <interpolation>
      <entry><ind> -5 </ind><dep> -202.5 </dep></entry>
      <entry><ind> -4 </ind><dep> -112.5 </dep></entry>
      <entry><ind> -3 </ind><dep>  -90.0 </dep></entry>
      <entry><ind> -1 </ind><dep>  -45.0 </dep></entry>
      <entry><ind>  0 </ind><dep>  -22.5 </dep></entry>
      <entry><ind>  1 </ind><dep>    0.0 </dep></entry>
      <entry><ind>  3 </ind><dep>   45.0 </dep></entry>
      <entry><ind>  5 </ind><dep>   90.0 </dep></entry>
      <entry><ind>  7 </ind><dep>  135.0 </dep></entry>
      <entry><ind>  9 </ind><dep>  180.0 </dep></entry>
      <entry><ind> 10 </ind><dep>  202.5 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </center> 
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
