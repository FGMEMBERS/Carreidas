<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <name>PFD 2d Panel</name>
  <background>Aircraft/Carreidas/Models/Interior/Panel/Instruments/PFD/transparent.png</background>
  <w>500</w>
  <h>400</h>

  <instruments>

    <instrument include="pfd-text.xml">
      <name>pfd-text</name>
      <condition>
          <greater-than>
              <property>systems/electrical/outputs/efis</property>
              <value>15</value>
          </greater-than>
      </condition>
      <x>250</x>
      <y>200</y>
      <w>500</w>
      <h>400</h>
    </instrument>
        
  </instruments>
  
</PropertyList>
