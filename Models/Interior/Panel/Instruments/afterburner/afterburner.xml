<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>afterburner.ac</path>

  <animation>
    <type>material</type>
    <object-name>Plane.001</object-name>
    <emission>
      <red>   1 </red>
      <green> 0 </green>
      <blue>  0 </blue>
      <factor-prop>controls/engines/engine/augmentation</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>Plane.002</object-name>
<visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-toggle</command>
      <property>controls/engines/engine[0]/augmentation</property>
    </binding>
    <binding>
      <command>property-toggle</command>
      <property>controls/engines/engine[1]/augmentation</property>
    </binding>
    <binding>
      <command>property-toggle</command>
      <property>controls/engines/engine[2]/augmentation</property>
    </binding>
  </action>
  </animation>

</PropertyList>

